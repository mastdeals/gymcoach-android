<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="gymcouch.main"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="7" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".GymCouch"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Configurar"
				  android:theme="@android:style/Theme.Dialog"
				  android:label="@string/lblConfigurar">
		</activity>
		<activity android:name="gymcouch.perfil.Perfil"
				  android:theme="@android:style/Theme.Dialog"
				  android:label="@string/lblPerfil">
		</activity>
		<activity android:name="gymcouch.ejercicios.Ejercicios"
				  android:theme="@android:style/Theme.Dialog"
				  android:label="@string/lblEjercicios">
		</activity>
		<activity android:name="gymcouch.ejercicios.Ejercicio"
				  android:theme="@android:style/Theme"
				  android:label="@string/lblEjercicios">
		</activity>
		<activity android:name="gymcouch.musculos.Musculos"
				  android:theme="@android:style/Theme"
				  android:label="@string/lblEjercicios">
		</activity>
    </application>
</manifest>